@using WebApplication.Models.Application
@model IEnumerable<WebApplication.Models.Application.RefurbRequest>

<div class="small-box StatusPrior-4">
    <div class="inner">
        <h3>@ViewBag.fi</h3>
        <p>Waiting Final Inspection</p>
    </div>
    <div class="icon">
        <i class="ion ion-android-send"></i>
    </div>
    <a href="@Url.Action("ByStatus", "RefurbRequest", new { status = RequestFlowStatus.SentToFinalInspection })" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
</div>


@RenderPage("~/Views/Home/_ListRequests.cshtml", new { actionName = "FinalInspection" })
