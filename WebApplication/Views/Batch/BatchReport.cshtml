@model WebApplication.Models.Application.Batch

@{
    ViewBag.Title = "Batch Report";
}

@section submenu {left-sidebar-menu-batch-@ViewBag.Menu}


<h2>Batch  # @Model.Id</h2>
<div class="row">
    <div class="col-md-12">
        <div class="box box box-primary">
            <div class="box-header with-border">
                <div class="box-body">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>Lote nº</th>
                                <th>Status</th>
                                <th>Quantidade do Lote</th>
                                <th>Quantidade Aprovada</th>
                                <th>Quantidate Reprovada - Functional Test</th>
                                <th>Quantidate Reprovada - Cosmetic</th>
                                <th>Quantidate Total Conferida</th>
                                <th>Data do Relatório</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td># @Model.Id</td>
                                <td>@Model.Status</td>
                                <td>@Model.Quantity</td>
                                <td>@Model.QuantityApproved</td>
                                <td>@Model.QuantityDisapproved</td>
                                <td>@Model.QuantityDisapprovedByCosmetic</td>
                                <td><strong>@Model.QuantityConferred</strong></td>
                                <td>@DateTime.Now</td>
                            </tr>
                        </tbody>
                    </table>

                    <div class="box-body">
                        <a href="@Url.Action("Details", "Batch", new { id = @Model.Id})" class="btn btn-default">
                          Go Back
                        </a>
                    </div>


                </div>

            </div>
         </div>
    </div>
</div>



              

            




